{
    "name": "nonmem scinteco",
    "displayName": "nonmem scinteco",
    "description": "nonmem syntax highlighter, Code block folding, Completion",
    "version": "0.2.0",
    "publisher": "com.scinteco",
    "homepage": "https://github.com/robert-boulanger/nonmem_extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/robert-boulanger/nonmem_extension"
    },
    "engines": {
        "vscode": "^1.72.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "icon.png",
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:nonmem"
    ],
    "contributes": {
        "languages": [
            {
                "id": "nonmem",
                "aliases": [
                    "nonmem"
                ],
                "extensions": [
                    ".mod",
                    ".ctl",
                    ".clt",
                    ".lst"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nonmem",
                "scopeName": "nonmem",
                "path": "./syntaxes/nonmem.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "nonmem_cn",
                "path": "./snippets/nonmem.json"
            }
        ]
    }
}
